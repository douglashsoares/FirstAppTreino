<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShowData">

    <TextView
        android:id="@+id/txtNomeApr"
        android:text="OlÃ¡  Senhor "
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtNomeInt"
        app:layout_constraintBottom_toTopOf="@+id/txtEmailApr"
        />

    <TextView
        android:id="@+id/txtNomeInt"
        android:text="Nome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtNomeApr"
        app:layout_constraintBottom_toTopOf="@+id/txtEmailInt"
        />

    <TextView
        android:id="@+id/txtEmailApr"
        android:text="Email : "
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/txtNomeApr"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtEmailInt"
        app:layout_constraintBottom_toTopOf="@+id/txtPassApr"


        />
    <TextView
        android:id="@+id/txtEmailInt"
        android:text="email"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNomeInt"
        app:layout_constraintStart_toEndOf="@+id/txtEmailApr"
        app:layout_constraintBottom_toTopOf="@+id/txtPassInt"
        />
    <TextView
        android:id="@+id/txtPassApr"
        android:text="Password:  "
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEmailApr"
        app:layout_constraintEnd_toStartOf="@+id/txtPassInt"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/txtPassInt"
        android:text="Senha"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEmailInt"
        app:layout_constraintStart_toEndOf="@+id/txtPassApr"
        />


</androidx.constraintlayout.widget.ConstraintLayout>